	}

	void Fourteen() {
		System.out.println("14번문제(계산문제)");
		System.out.println("6 / 2 + 3 X 5 =?");
		System.out.println("1.3 2.4 3.5 4.18 5.9");
		Scanner a = new Scanner(System.in);
		int sum;
		A b = new A();
		Error e = new Error();
		while ((sum = a.nextInt()) != 0) {
			switch (sum) {
			case 4 -> End();
			case 3, 2, 5, 1 -> e.Q14();
			}
		}
	}

	void End() {
		System.out.println("이제 '14일 동안 건강해지기 프로젝트'가 끝났어. 그동안 생활 습관 바꾸느라 수고 많았어.");
		System.out.println("하지만 앞으로도 이 습관을 유지해야돼. 그렇지 않으면 건강은 금방 다시 망가지고 말거야.");
		System.out.println("너의 목숨이 위험할 정도로 건강이 나빠져야 내가 다시 나올 수 있겠지만, 난 네 눈 앞에 다시 나오고 싶지 않아.");
		System.out.println("앞으로 건강 관리 잘해서 만날 일 없게 하자!! 파이팅!");
	}
}

