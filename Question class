class Problems {
	void One() {
		System.out.println("1번문제");
		System.out.println("인공지능학부 입학 정원은?(정답 번호만 입력)");
		System.out.println("1.50명 2.40명 3.30명 4.20명 5.10명");
		Scanner a = new Scanner(System.in);
		int sum;
		A b = new A();
		Error e = new Error();
		while ((sum = a.nextInt()) != 0) {
			switch (sum) {
			case 1 -> b.DayTwo();
			case 2, 3, 4, 5 -> e.Q1();
			}
		}
	}

	void Two() {
		System.out.println("2번문제");
		System.out.println("객지프 교수님의 성함은?");
